rules_version = '2';
service firebase.storage {
  match /b/ifaa-54ebf.appspot.com/o {
    match /{allPaths=**} {
      allow read, write: if request.auth != null;
    }
    
    // Add CORS headers to allow web access
    match /news_images/{imageId} {
      allow read, write: if request.auth != null;
    }
  }
}